import random

def mystery_function(x, y):
    random_number = random.randint(0,2) # What's the range of randoms?
    if random_number > 0: # What's the probability that random_number is greater than 0?
        z = x + y
    else:
        z = x * y
    print z
mystery_function(1, 2)

#When you end with return it does not do anything because  is not an output
#Otherwise when you press print you end up with three and two because you add and multiply x and y.
